{
    "extends": [
        "airbnb",
        "prettier",
        "prettier/react"
    ],
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": [
            "error",
            {
              "singleQuote": true
            }
        ],
        "react/jsx-filename-extension": [
            1,
            { "extensions": [".js", ".jsx"] }
        ]
    }
}